{
  "name": "@oshankhz/n8n-nodes-bee2bee-indexer",
  "version": "0.1.0",
  "description": "n8n node to index GitHub repositories and generate embeddings for RAG",
  "keywords": [
    "n8n-community-node-package",
    "github",
    "indexer",
    "embeddings",
    "rag",
    "vector",
    "ai"
  ],
  "license": "MIT",
  "homepage": "https://github.com/OshanKHZ/bee2bee-indexer-repo",
  "author": {
    "name": "Bee2Bee Team",
    "email": "team@bee2bee.ai"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OshanKHZ/bee2bee-indexer-repo.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "restricted"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/Bee2BeeIndexerApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Bee2BeeIndexer/Bee2BeeIndexer.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.16.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^5.0.1",
    "n8n-core": "*",
    "n8n-workflow": "*",
    "prettier": "^2.7.1",
    "typescript": "~5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
